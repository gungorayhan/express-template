{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars":"off",
        "no-empty":"off",
        "no-undef":"off",
        "no-const-assign":"off"
    }
}
